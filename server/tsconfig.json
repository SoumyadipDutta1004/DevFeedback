{
  "compilerOptions": {
      "module": "CommonJS",
      "esModuleInterop": true,
      "target": "ESNext",
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "sourceMap": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}